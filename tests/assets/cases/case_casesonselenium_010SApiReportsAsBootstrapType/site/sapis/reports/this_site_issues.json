{
	"api": "this_site",
	"service": "issues",
	"listPath": "data/toobasic",
	"params": [
		"githubresult"
	],
	"columns": [
		{
			"title": "ID",
			"path": "html_url",
			"type": "link",
			"label": "Go to",
			"label_field": "number",
			"attrs": {
				"target": "_blank"
			}
		}, {
			"title": "@title_state",
			"path": "state",
			"exclude": ["open"]
		}, {
			"title": "Assignee",
			"path": "assignee/html_url",
			"type": "button-link",
			"label_field": "assignee/login",
			"attrs": {
				"class": "btn-xs btn-danger"
			}
		}, {
			"title": "Reporter",
			"path": "user/html_url",
			"type": "button-link",
			"label_field": "user/login",
			"attrs": {
				"class": "btn-xs btn-info"
			}
		}, {
			"title": "@title_avatar",
			"path": "user/avatar_url",
			"type": "image",
			"attrs": {
				"style": {
					"width": "24px"
				}
			}
		}, {
			"title": "Description",
			"type": "code",
			"path": "body",
			"attrs": {
				"data-some": "value"
			}
		}
	],
	"exceptions": [
		{
			"path": "pull_request",
			"isset": true
		}, {
			"path": "number",
			"exclude": [
				122,
				123
			]
		}
	],
	"attrs": {
		"class": "InternalClass"
	}
}
